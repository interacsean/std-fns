{
  "name": "std-fns",
  "version": "0.2.2",
  "description": "Functional JavaScript Library",
  "main": "index.js",
  "repository": "https://github.com/dandro/std-fns",
  "author": "dnlmrtnz",
  "license": "MIT",
  "scripts": {
    "test": "jest ./src",
    "check": "eslint ./src && flow ./src",
    "build": "npm run check && babel src/ -d ./ --ignore **/*spec.js && npm run build:docs && npm run build:types",
    "build:types": "flow-copy-source -v -i **/*spec.js src ./",
    "build:docs": "node ./scripts/docs"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "eslint": "5.2.0",
    "eslint-plugin-flowtype": "2.47.1",
    "flow-bin": "0.72.0",
    "flow-copy-source": "^2.0.2",
    "jest": "22.4.4",
    "jsdoc-to-markdown": "^4.0.1"
  }
}
